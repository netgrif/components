{
    "name": "@netgrif/application-engine-project",
    "version": "5.7.2",
    "description": "Netgrif Application Engine Frontend project. Project includes angular library as base for NAE applications.",
    "homepage": "https://netgrif.com",
    "repository": {
        "type": "git",
        "url": "https://bitbucket.org/netgrif/nae-frontend.git"
    },
    "bugs": {
        "url": "https://netgrif.atlassian.net/browse/NAE"
    },
    "keywords": [
        "netgrif",
        "application engine",
        "nae",
        "engine",
        "workflow engine"
    ],
    "contributors": [
        "Milan Mladoniczky <mladoniczky@netgrif.com>",
        "Juraj Mažári <mazari@netgrif.com>",
        "Tomáš Kováčik <kovacik@netgrif.com>",
        "Jakub Kovář <kovar@netgrif.com>",
        "Martin Miklovič <miklovic@netgrif.com>",
        "Daniel Vaník <vanik@netgrif.com>",
        "Lukáš Rezák <rezak@netgrif.com>",
        "Jozef Daxner <daxner@netgrif.com>",
        "Štefan Renczés <renczes@netgrif.com>"
    ],
    "scripts": {
        "ng": "ng",
        "nae:build": "ng build netgrif-application-engine --prod && npm run nae:build-schematics",
        "nae:build-schematics": "node projects/netgrif-application-engine/src/scripts/build-schematics.js",
        "nae:lint": "ng lint netgrif-application-engine",
        "nae:test": "ng test netgrif-application-engine",
        "nae:full-test": "npm run nae:lint && npm run nae:test",
        "nae:normalize-coverage-summary": "node scripts/normalize-file-paths.js coverage/netgrif-application-engine/coverage-summary.json",
        "nae:typedoc": "typedoc --tsconfig projects/netgrif-application-engine/tsconfig.lib.json  projects/netgrif-application-engine/src/lib",
        "nae:doc": "npm run nae:normalize-coverage-summary && compodoc -c projects/netgrif-application-engine/.compodocrc.json",
        "nae:local-build": "npm run nae:build && npm i dist/netgrif-application-engine --save-optional",
        "nae:prod-build": "npm run nae:full-test && npm run nae:sonar && npm run nae:build && npm run nae:doc",
        "nae:prod-build-without-tests": "ng lint netgrif-application-engine && npm run nae:sonar && npm run nae:build && npm run nae:doc",
        "nc:clean": "node projects/netgrif-components/src/scripts/delete-local-build-link.js",
        "nc:build": "npm run nc:clean && ng build netgrif-components --prod && npm run nc:build-schematics",
        "nc:local-build": "npm run nc:build && npm i dist/netgrif-components --save-optional",
        "nc:build-schematics": "node projects/netgrif-components/src/scripts/build-schematics.js",
        "nc:lint": "ng lint netgrif-components",
        "nc:test": "ng test netgrif-components",
        "nc:full-test": "npm run nc:lint && npm run nc:test",
        "nc:normalize-coverage-summary": "node scripts/normalize-file-paths.js coverage/netgrif-components/coverage-summary.json",
        "nc:doc": "npm run nc:normalize-coverage-summary && compodoc -c projects/netgrif-components/.compodocrc.json",
        "nc:compodoc": "compodoc -c projects/netgrif-components/.compodocrc.json",
        "project:local-build": "npm run nae:local-build && npm run nc:local-build",
        "project:full-test": "npm run nae:full-test && npm run nae:local-build && npm run nc:full-test",
        "project:start": "npm run project:local-build && ng serve",
        "project:sonar": "node scripts/sonar-scanner-analysis.js",
        "components:start": "npm run nc:local-build && ng serve",
        "example:start": "ng serve",
        "example:build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "postversion": "node scripts/copy-version.js"
    },
    "private": true,
    "dependencies": {
        "@angular-material-components/datetime-picker": "~4.0.5",
        "@angular-material-components/moment-adapter": "~4.0.1",
        "@angular/animations": "~10.2.3",
        "@angular/cdk": "~10.2.7",
        "@angular/common": "~10.2.3",
        "@angular/compiler": "~10.2.3",
        "@angular/core": "~10.2.3",
        "@angular/flex-layout": "~10.0.0-beta.32",
        "@angular/forms": "~10.2.3",
        "@angular/material": "~10.2.7",
        "@angular/material-moment-adapter": "~10.2.7",
        "@angular/platform-browser": "~10.2.3",
        "@angular/platform-browser-dynamic": "~10.2.3",
        "@angular/router": "~10.2.3",
        "@covalent/core": "~3.1.0",
        "@covalent/highlight": "~3.1.0",
        "@covalent/markdown": "~3.1.0",
        "@covalent/text-editor": "~3.1.0",
        "@ngx-translate/core": "~13.0.0",
        "@ngx-translate/http-loader": "~6.0.0",
        "@schematics/angular": "~10.2.0",
        "@swimlane/ngx-charts": "~16.0.0",
        "angular-resizable-element": "~3.3.0",
        "angular-resize-event": "~2.0.1",
        "angular2-hotkeys": "~2.2.0",
        "@types/mousetrap": "1.6.3",
        "hammerjs": "~2.0.8",
        "json-schema-to-typescript": "~8.1.0",
        "jszip": "~3.2.2",
        "moment": "~2.24.0",
        "natural-orderby": "~2.0.3",
        "ngx-quill": "~12.0.1",
        "palette-creator": "~0.5.4",
        "quill": "~1.3.7",
        "rxjs": "~6.5.4",
        "tslib": "~1.11.1",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1002.0",
        "@angular-devkit/build-ng-packagr": "~0.1002.0",
        "@angular-devkit/schematics-cli": "~0.1002.0",
        "@angular/cli": "~10.2.0",
        "@angular/compiler-cli": "~10.2.3",
        "@angular/language-service": "~10.2.3",
        "@compodoc/compodoc": "1.1.13",
        "@types/jasmine": "~3.5.9",
        "@types/jasminewd2": "~2.0.3",
        "@types/moment": "~2.13.0",
        "@types/node": "~12.12.29",
        "codelyzer": "~5.2.1",
        "commander": "~4.1.1",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "json-server": "~0.15.1",
        "karma": "~4.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-firefox-launcher": "~1.3.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "~1.5.2",
        "karma-junit-reporter": "~2.0.1",
        "karma-mocha-reporter": "~2.2.5",
        "karma-nyan-reporter": "~0.2.5",
        "ncp": "~2.0.0",
        "ng-packagr": "~9.0.3",
        "protractor": "~5.4.3",
        "puppeteer": "~2.1.1",
        "schematics-utilities": "~2.0.1",
        "sonarqube-scanner": "~2.5.0",
        "ts-node": "~8.3.0",
        "tslint": "~5.18.0",
        "typescript": "~4.0.2",
        "typescript-json-schema": "~0.42.0",
        "unique-names-generator": "~4.2.0"
    },
    "optionalDependencies": {
        "@netgrif/application-engine": "file:dist/netgrif-application-engine",
        "@netgrif/components": "file:dist/netgrif-components"
    }
}
