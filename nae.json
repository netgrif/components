    "providers": {
        "auth": {
            "address": "http://localhost:8080/api/",
            "authentication": "Basic",
            "endpoints": {
                "login": "auth/login",
                "logout": "auth/logout",
                "signup": "auth/signup",
                "verification": "auth/verify",
                "verify": "auth/token/verify",
                "invite": "auth/invite",
                "reset": "auth/reset",
                "recover": "/auth/recover"
            },
            "sessionBearer": "X-Auth-Token",
            "jwtBearer": "X-Jwt-Token"
        },
        "resources": [
            {
                "name": "case",
                "address": "http://localhost:8080/api/",
                "format": "hal",
